pipeline {
    agent any
    stages {
        stage('Deploy pacakage to S3') {
            steps {
               
                sh 'ls -ltrh'
                sh 'bash ./express-demo.sh'
                
             
            }
        }
        stage('Download package from s3') {
            steps {
                
               
                sh 'node server.js &'
            }
        }
    }
}
